sqle:
  id: 1
  dms_server_address: http://127.0.0.1:10000
  api:
    addr: 127.0.0.1
    port: 10001
    enable_https: false
    cert_file_path: './etc/cert.pem'
    key_file_path: './etc/key.pem'
  secret_key:
  service:
    auto_migrate_table: true
    debug_log: false
    log_path: './logs'
    log_max_size_mb: 1024
    log_max_backup_number: 2
    plugin_path: './plugins'
    enable_cluster_mode:
    database:
      mysql_host: '127.0.0.1'
      mysql_port: '3306'
      mysql_user: 'root'
      mysql_password: 'pass'
      mysql_schema: 'sqle'
